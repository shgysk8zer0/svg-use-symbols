{
  "name": "@shgysk8zer0/svg-use-symbols",
  "version": "1.0.0",
  "description": "An npm package for use with `<svg><use xlink:href=\"\"></use></svg>`",
  "private": false,
  "type": "module",
  "main": "generator.cjs",
  "module": "generator.mjs",
  "exports": {
    ".": {
      "import": "./generator.mjs",
      "require": "./generator.cjs"
    }
  },
  "bin": {
    "svg-use-symbols": "./cli.mjs"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "npm run lint:js && npm run build",
    "preversion": "npm test",
    "prepare": "npm run build",
    "lint:js": "eslint .",
    "fix:js": "eslint. --fix",
    "build": "npm run build:cli && npm run build:generator",
    "build:cli": "rollup -c cli.config.js",
    "build:generator": "rollup -c generator.config.js",
    "create:lock": "npm i --package-lock-only",
    "version:bump": "npm run version:bump:patch",
    "version:bump:patch": "npm version --no-git-tag-version patch",
    "version:bump:minor": "npm version --no-git-tag-version minor",
    "version:bump:major": "npm version --no-git-tag-version major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shgysk8zer0/svg-use-symbols.git"
  },
  "author": "Chris Zuber <admin@kernvalley.us>",
  "license": "MIT",
  "funding": [
    {
      "type": "librepay",
      "url": "https://liberapay.com/shgysk8zer0"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/shgysk8zer0"
    }
  ],
  "bugs": {
    "url": "https://github.com/shgysk8zer0/svg-use-symbols/issues"
  },
  "homepage": "https://github.com/shgysk8zer0/svg-use-symbols#readme",
  "devDependencies": {
    "@shgysk8zer0/js-utils": "^1.0.0"
  },
  "dependencies": {
    "@shgysk8zer0/npm-utils": "^1.0.5",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^10.0.1"
  }
}
